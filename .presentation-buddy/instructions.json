    "type": "typeText",
    "text": [
      "function createResource(fetch: (...args: any[]) => Promise<any>) {",
      "  let promise: Promise<any> | null = null;",
      "  let result: any = undefined;",
      "  let error: any = undefined;",
      "",
      "  async function executeFetch(...args: any[]) {",
      "    try {",
      "      result = await fetch(...args);",
      "    } catch (err) {",
      "      error = err;",
      "    }",
      "  }",
      "",
      "  return {",
      "    read(...args: any[]) {",
      "      if (!promise) {",
      "        promise = executeFetch(...args);",
      "      }",
      "",
      "      if (result !== undefined) {",
      "        return result;",
      "      } else if (error !== undefined) {",
      "        throw error;",
      "      } else {",
      "        throw promise;",
      "      }",
      "    }",
      "  };",
      "}",
      "",
      "export default createResource;"
    ]
  },
